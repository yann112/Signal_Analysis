    "N = len(X)\n",
    "#liste de 0 à N \n",
    "n = np.arange(N)\n",
    "# sampling rate 1 Hz\n",
    "sr = 1\n",
    "#temps en seconde 60\n",
    "T = N/sr\n",
    "#frequence\n",
    "freq = n/T\n",
